{
  "presets": [
    ["env", {"modules": false}],
     "react", 
     "stage-2"
    ],
  "plugins": [
    "react-hot-loader/babel",
    [
      "react-css-modules",
      {
        "filetypes": {
          ".scss": {
            "syntax": "postcss-scss",
            "plugins": [
              "postcss-nested"
            ]
          }
        },
        "generateScopedName": "[path]___[name]__[local]___[hash:base64:5]",
        "webpackHotModuleReloading": true
      }
    ]
  ]
}
