{
  "private": true,
  "workspaces": ["parts/*"],
  "scripts": {
    "be:dc": "docker-compose -f scripts/docker/docker-compose.backend.yml --project-directory .",
    "be:run": "yarn be:dc run backend bash",
    "be:exec": "yarn be:dc exec backend bash",
    "be": "yarn --cwd ./parts/backend",
    "fe": "yarn --cwd ./parts/frontend"
  },
  "_moduleAliases": {
    "@": "parts/backend/src",
    "#": "parts/frontend/src"
  }
}